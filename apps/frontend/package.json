{
  "name": "@epayco/frontend",
  "version": "0.1.0",
  "private": true,
  "description": "Vue 3 client for the wallet platform.",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx,.vue",
    "format": "prettier --write \"src/**/*.{ts,tsx,vue}\"",
    "test": "vitest --run"
  },
  "dependencies": {
    "@epayco/shared": "workspace:*",
    "axios": "^1.6.7",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-i18n": "^9.9.0",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/vue": "^8.0.2",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.21.1",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vite": "^5.1.5",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.29",
    "jsdom": "^24.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
