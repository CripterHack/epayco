{
  "name": "@epayco/wallet-db-service",
  "version": "0.1.0",
  "private": true,
  "description": "Domain service with exclusive access to the wallet database.",
  "scripts": {
    "build": "nest build",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "pnpm run typeorm -- migration:run -d src/database/data-source.ts",
    "migration:revert": "pnpm run typeorm -- migration:revert -d src/database/data-source.ts",
    "migration:show": "pnpm run typeorm -- migration:show -d src/database/data-source.ts",
    "seed:run": "ts-node --project tsconfig.json src/database/seeds/seed.ts"
  },
  "dependencies": {
    "@epayco/shared": "workspace:*",
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/typeorm": "^10.0.0",
    "argon2": "^0.40.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "joi": "^17.11.0",
    "mysql2": "^3.9.0",
    "nestjs-pino": "^3.4.0",
    "nodemailer": "^6.9.10",
    "pino": "^8.15.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.3.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.19",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "pino-pretty": "^10.2.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
